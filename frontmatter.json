{
  "$schema": "https://frontmatter.codes/frontmatter.schema.json",
  "frontMatter.content.pageFolders": [
    {
      "title": "content",
      "path": "[[workspace]]/content"
    },
    {
      "title": "blog",
      "path": "[[workspace]]/content/blog"
    },
    {
      "title": "other",
      "path": "[[workspace]]/content/other"
    },
    {
      "title": "redkendoc",
      "path": "[[workspace]]/content/redkendoc"
    }
  ],
  "frontMatter.taxonomy.contentTypes": [
    {
      "name": "default",
      "previewPath": null,
      "pageBundle": false,
      "fields": [
        {
          "title": "layout",
          "name": "layout",
          "type": "string"
        },
        {
          "title": "author",
          "name": "author",
          "type": "choice",
          "choices": [
            "Pablo Iranzo GÃ³mez",
            "Enrique Belarte"
          ]
        },
        {
          "title": "title",
          "name": "title",
          "type": "string"
        },
        {
          "title": "tags",
          "name": "tags",
          "type": "tags"
        },
        {
          "title": "date",
          "name": "date",
          "type": "datetime"
        },
        {
          "title": "categories",
          "name": "categories",
          "type": "categories"
        },
        {
          "title": "modified",
          "name": "modified",
          "type": "datetime"
        },
        {
          "title": "redirect",
          "name": "redirect",
          "type": "string"
        }
      ]
    }
  ],
  "frontMatter.framework.id": "hugo",
  "frontMatter.content.publicFolder": "static",
  "frontMatter.preview.host": "http://localhost:1313"
}
